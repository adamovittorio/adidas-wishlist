{
  "name": "adidas-wishlist",
  "version": "1.0.0",
  "description": "a simple wishlist",
  "main": "src",
  "scripts": {
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage; npx http-server ./coverage/lcov-report -o",
    "start:server": "npx nodemon src/server",
    "start:client": "npx webpack-dev-server --progress --color --open --config ./webpack.dev.js",
    "start:dev": "concurrently --kill-others \"npm run start:server\" \"npm run start:client\"",
    "build": "npx webpack --config webpack.prod.js",
    "serve": "npm run build; npx serve -s -C --open ./public",
    "start": "node src/server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamovittorio/adidas-wishlist.git"
  },
  "keywords": [
    "adidas",
    "wishlist",
    "graphql",
    "react",
    "apollo"
  ],
  "author": "Vittorio Adamo <vittorio.adamo@arreeba.com> (https://ristoranti.it/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adamovittorio/adidas-wishlist/issues"
  },
  "homepage": "https://github.com/adamovittorio/adidas-wishlist#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "clean-webpack-plugin": "0.1.18",
    "concurrently": "3.5.1",
    "eslint": "4.16.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "html-webpack-plugin": "2.30.1",
    "http-server": "0.11.1",
    "jest": "22.1.4",
    "nodemon": "1.14.11",
    "react-hot-loader": "3.1.3",
    "serve": "6.4.9",
    "uglifyjs-webpack-plugin": "1.1.8",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1",
    "webpack-merge": "4.1.1"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.1.5",
    "apollo-client": "2.2.1",
    "apollo-link-http": "1.3.2",
    "apollo-server-express": "1.3.2",
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "express": "4.16.2",
    "graphql": "0.12.3",
    "graphql-tag": "2.6.1",
    "graphql-tools": "2.19.0",
    "lodash": "4.17.4",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-apollo": "2.0.4",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2",
    "redux-thunk": "2.2.0",
    "styled-components": "3.1.4"
  }
}
